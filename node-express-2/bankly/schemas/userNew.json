{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
      "username": {
        "type": "string"
      },
      "password": {
        "type": "string"
      },
      "first_name": {
        "type": "string"
      },
      "last_name": {
        "type": "string"
      },
      "email": {
        "type": "string",
        "format": "email"
      },
      "phone": {
        "type":  "string"
      },
      "admin": {
        "type": "boolean"
      }
    },
    "additionalProperties": false,
    "required": [
      "username",
      "first_name",
      "last_name",
      "password",
      "email",
      "phone"
    ]
}